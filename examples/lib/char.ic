Ascii ::= struct { _val: nat8 }

ascii ::= (val: nat8) -> Ascii {
  c: Ascii
  c._val = val
  return c
}

(print) ::= (c: Ascii) -> {
  putchar ::= foreign("putchar", nat8 -> int32)
  putchar(c._val)
}

is_eof ::= (c: Ascii) => c._val == 255 as nat8

read ::= () -> Ascii {
  getchar ::= foreign("getchar", () -> int32)
  c := ascii(getchar() as nat8)
  return c
}
