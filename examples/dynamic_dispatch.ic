main ::= void -> void {
  x: bool | int = 999
  y: bool | int = true

  foo(3, 4)
  x = true
  y = 123
  foo(x, y)

  x = true
  y = false
  foo(x, y)

  x = 123
  y = true
  foo(x, y)

  x = 456
  y = 789
  foo(x, y)
}

foo ::= (x: bool, y: bool) -> void { print "foo(x: bool, y: bool)\n" }
foo ::= (x: bool, y: int) -> void { print "foo(x: bool, y: int)\n" }
foo ::= (x: int, y: bool) -> void { print "foo(x: int, y: bool)\n" }
foo ::= (x: int, y: int) -> void { print "foo(x: int, y: int)\n" }
