main := void -> void {
  print_type_info := foo
  
  foo := (val : Any`T) -> void {
    blank: T
    print blank
    print ": "
    print T
    print '\n'
  }

  print_type_info('x')
  print_type_info(3)
  print_type_info(3u)
  print_type_info(3.0)
  print_type_info(p)

  p: pie

}

Any := (T: type) -> bool {
  return true
}

pie := struct {
  pi := 3.14159
  e := 2.71828
}

__print__ := (p: pie) -> void {
  print "("
  print p.pi
  print ", "
  print p.e
  print ")"
}
