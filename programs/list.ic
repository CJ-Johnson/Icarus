list := struct(T: type) {
  size: uint
  head: &node(T)
}


node := struct(T: type) {
  data: T
  next: &node(T)
}

__print__ := (l: list(int)) -> void {
  iter := l.head
  if l.size == 0u {
    return
  }
  print '['
    i := 0u
    while i < l.size {
      i += 1u
      print iter.data
      if i != l.size {
        print " -> "
      }
      iter = iter.next
    }
  
  print ']'
}

