}

add := (l: &list(int), n: int) -> void {
  nptr := alloc(node(int))
  nptr.data = n
  nptr.next = l.head
  l.head = nptr
  l.size += 1u
}
add := (l: &list(char), c: char) -> void {
  nptr := alloc(node(char))
  nptr.data = c
  nptr.next = l.head
  l.head = nptr
  l.size += 1u
}
add := (l: &list(list(char)), c: list(char)) -> void {
  nptr := alloc(node(list(char)))
  nptr.data = c
  nptr.next = l.head
  l.head = nptr
  l.size += 1u
}
