main := void -> void {
  l: list(int)
  add(&l, 3)
  add(&l, 5)
  add(&l, 7)

  print l
  print '\n'

  emptylist: list(int)

  print emptylist
  print '\n'

  l1: list(char)
  add(&l1, 'o')
