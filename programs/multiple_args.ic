f := (a : int, b : int) -> int {
  return a * a + b * b
}

main := void -> void {
print f(3, 4)
print `n
print f(0-3, 4)
print `n
print f(10, 0)
print `n
print g(1, 2, 3, 4, 3, 2, 1)
print `n
print g(1, 2, 2, 4, 3, 2, 1)
print `n
print g(1, 2, 2, 0 - 1, 4, 2, 1)
}

g := (a: int, b: int, c: int, d: int, e: int, h: int, i: int) -> int {
  return case {
    a > b  =>  a
    b > c  =>  b
    c > d  =>  c
    d > e  =>  d
    e > h  =>  e
    h > i  =>  h
    else   =>  i
  }
}
