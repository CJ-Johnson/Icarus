// Segfaults because strings are not yet incorporated
water_state := (temp : int) -> char {
   return case {
    temp < 32         =>  's'
    32 <= temp < 212  =>  'l'
    else              =>  'g'
  }
}

main := void -> void {
  print water_state(0)
  print water_state(10)
  print water_state(100000)
  print water_state(50)
}
