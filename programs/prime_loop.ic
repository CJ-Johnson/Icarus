
main := void -> void {
num_primes := 10000
m := 2

while num_primes > 0 {
  if is_prime(m) {
    print m
    print ','
    print ' '
    num_primes -= 1
  }
  
  m += 1
}
}

is_prime := (n : int) -> bool {
  k := 2
  b := true

  while k * k <= n {
    b &= n % k != 0
    k += 1
  }

  return b
}

print '\n'
