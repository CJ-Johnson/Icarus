hello_world := void -> void {
  print  'H', 'e', 'l', 'l', 'o', ' ', 'W', 'o', 'r', 'l', 'd', '!', '\n'
}

fibonacci := (n: int) -> int {
  return case {
    n == 0 => 0
    n == 1 => 1
    else   => fibonacci(n - 1) + fibonacci(n - 2)
  }
}

main := void -> void {
  fib_of_20 := void -> int { return fibonacci(20) }

  {{ hello_world() }}

  print {{ fib_of_20() }}
}
