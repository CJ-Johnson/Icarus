main := void -> void {
  print fourth(1.1)
  print '\n'
  print a(20)
}

square := (r: real) -> real {
  return r * r
}

fourth := square * square

c := (n: int) -> real {
  return (n :> real) + 0.5
}
d := (r: real) -> int {
  x := r * r
  return case {
    x < 1.0        => 0
    1.0 <= x < 2.0 => 1
    2.0 <= x < 3.0 => 2
    else           => 3
  }
}

b := c * d
a := b * c
