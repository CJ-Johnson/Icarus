pair := struct {
  x := square(4) + 1
  y := 'x'
  z : int
}

__print__ := (p: pair) -> void {
  print '('
  print p.x
  print ','
  print p.y
  print ','
  print p.z
  print ')'
}

square := (n: int) -> int {
  return n * n
}

main := void -> void {
  func := (num: int) -> int {
    return 17 * num
  }
  
  p: pair
  f: foo
  print p
  print '\n'
  print f
  print '\n'

  foo := struct {
    n := func(4)
  }

  __print__ := (a: foo) -> void {
    print a.n
  }
}
