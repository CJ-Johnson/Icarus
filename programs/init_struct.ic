triple := struct {
  x := square(4) + 1
  y := 'x'
  z : int
}

__print__ := (t: triple) -> void {
  print `(, t.x, `,, t.y, `,, t.z, `)
}

square := (n: int) -> int {
  return n * n
}

func := (num: int) -> int { return 17 * num }

main := void -> void {
  __print__ := (a: foo) -> void { print a.n }

  t: triple
  f: foo
  print t, `n
  print f, `n

  foo := struct {
    n := func(5)
  }

}
