fib : int -> int
fib = (n : int) -> int {
   return case {
     n < 0 & n % 2 == 0  =>  fib(0 - n)
     n < 0               =>  0 - fib(0 - n)
     n < 2               =>  n
     else                =>  fib(n - 1) + fib(n - 2)
  }
}

i := 0 - 30
while i < 30 {
  print i
  print ':'
  print ' '
  print fib(i)
  print '\n'
  i += 1
}
