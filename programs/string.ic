__print__ := (s: string) -> void {
  i := 0
  while i < s.length {
    print s.chars[i]
    i += 1
  }
}


string := type {
  chars: [100; char]
  length: int
}

main := void -> void {
  s: string
  s.chars[0] = 'h'
  s.chars[1] = 'e'
  s.chars[2] = 'l'
  s.chars[3] = 'l'
  s.chars[4] = 'o'
  s.chars[5] = '!'
  s.length = 6
  print s
  return
}
