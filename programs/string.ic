__print__ := (s: string) -> void {
  i := 0
  while i < s.length {
    print s.chars[i]
    i += 1
  }
}

hello := void -> string {
  s: string
  s.length = 7
  s.chars = ['h', 'e', 'l', 'l', 'o', '!','\n']
  return s
}

string := type {
  chars: [100; char]
  length: int
}

main := void -> void {
  x : string = hello()
  print x
  s := hello()
  print s
  print hello()
  return
}
