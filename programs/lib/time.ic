clock: void -> uint #cstdlib
sleep: int -> void #cstdlib

Time := struct {
  _val: uint // In nanoseconds
}

Duration := struct {
  _val: uint // In nanoseconds
}

Now := void -> Time {
  result: Time = --
  result._val = clock()
  return result
}

__sub__ := (lhs: Time, rhs: Time) -> Duration {
  result: Duration = --
  result._val = lhs._val - rhs._val
  return result
}

__print__ := (t: Time) -> void { print t._val, "ns time" }
__print__ := (d: Duration) -> void { print d._val, "ns" }
