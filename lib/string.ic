string := type {
  chars: [100; char]
  length: uint
}

__print__ := (s: string) -> void {
  i := 0
  int_len := s.length :> int
  while i < int_len {
    print s.chars[i]
    i += 1
  }

//  for c in s.chars[ .. s.length - 1] {
//    print c
//  }
}

size := (s: string) -> uint {
  return s.length
}
